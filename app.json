{
  "expo": {
    "name": "Starterpack Breakfast",
    "slug": "starterpack",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#ffffff",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starterpackgh.order",
      "buildNumber": "13",
      "infoPlist": {
	    "NSLocationWhenInUseUsageDescription": "Starterpack Breakfast needs to access your location to automatically set the delivery address."
	  }
    },
    "androidNavigationBar": {
     
    "visible": true,

    "barStyle": "dark-content",

    "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.starterpackgh.order",
      "versionCode": 13
    },
    "androidStatusBar": {
    "barStyle": "light-content",
    "backgroundColor": "#ffffff"
  }
  }
}